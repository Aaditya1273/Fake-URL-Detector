{
  "version": 2,
  "builds": [
    {
      "src": "api/server.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "FLASK_APP": "index.py",
    "FLASK_ENV": "production",
    "PYTHONPATH": ".",
    "FLASK_DEBUG": "0"
  }
}
